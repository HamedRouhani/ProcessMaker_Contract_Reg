de\").show();\n    $(\"#Contract_Title\").show();\n    break;\n    \n  case (CA == 1 && SRT == 1) :\n    $(\"#Amendment_Code\").show();\n    $(\"#Amendment_Title\").show();\n    $(\"#Contract_Code\").show();\n    $(\"#Contract_Title\").show();\n    break; \n    \n  case (CA == 1 && SRT != 1) :\n    $(\"#Amendment_Code\").show();\n    $(\"#Amendment_Title\").show();\n    $(\"#Contract_Code\").hide();\n    $(\"#Contract_Title\").hide();\n    break;    \n    \n}\n\n// تفكيك نمايش ايجاد / اصلاح / حذف\n\n$(\"#Contract_Status_Text\").hide();\n\nswitch (true) {\n  \n  case ($(\"#Select_Reg_Type\").getValue() == 1 && $(\"#Contract_Amendment\").getValue() == 0):\n    $(\"#Subject_Show\").hide();\n    $(\"#Suppliers_Show\").hide();\n    $(\"#Personnel_Show\").hide();\n    $(\"#Projects_Show\").hide();\n    $(\"#Amount_Show\").hide();\n    $(\"#Location_Show\").hide();\n    break;\n    \n  case ($(\"#Select_Reg_Type\").getValue() == 3):\n    $(\"#Subject\").hide();\n    $(\"#Suppliers\").hide();\n    $(\"#Personnel\").hide();\n    $(\"#Projects\").hide();\n    $(\"#Amount\").hide();\n    $(\"#Location\").hide();\n    $(\"#Contract_Status_Text\").show();\n    $(\"#Contract_Status\").hide();\n    break;    \n    \n};\n\n\n//پر كردن شرح و توضيحات  --- \n\nif ($(\"#Description\").getNumberRows() == 1 && $(\"#Description\").getValue (1,1) ==\"\") {\n\t$('#Description').getControl($(\"#Description\").getNumberRows(),'2').attr('disabled',true);\n  \t$(\"#Description\").setValue($(\"#txt_name\").getValue(),$(\"#Description\").getNumberRows(),'1');\n  }else{\n\t$(\"#Description\").addRow();\n\t$('#Description').getControl($(\"#Description\").getNumberRows(),'2').attr('disabled',true); \n    $(\"#Description\").setValue($(\"#txt_name\").getValue(),$(\"#Description\").getNumberRows(),'1');\n  };\n\n// تعيين وضعيت توضيحات\n\nvar RC = $(\"#Reg_Confirmation\").getValue();\nvar SRT = $(\"#Select_Reg_Type\").getValue();\n  \nswitch (true) {\n    \n    case (RC == 0 && SRT == 1):\n      $('#Description').getControl($(\"#Description\").getNumberRows(),2).css(\"background-Color\", \"lightgreen\");\n      $(\"#Description\").setValue(\"اطلاعات ايجاد شده مربوط به قرارداد / الحاقيه مردود شد و ثبت نگرديد .\",$(\"#Description\").getNumberRows(),2);\n      break;     \n      \n    case (RC == 1 && SRT == 1):\n      $('#Description').getControl($(\"#Description\").getNumberRows(),2).css(\"background-Color\", \"lightgreen\");\n      $(\"#Description\").setValue(\"اطلاعات ايجاد شده مربوط به قرارداد / الحاقيه تاييد شد و ثبت گرديد .\",$(\"#Description\").getNumberRows(),2);\n      $('#Reg_Confirmation').find(\"span.textlabel\").css(\"background-color\", \"\");\n      break;  \n      \n    case (RC == 0 && SRT == 2):\n      $('#Description').getControl($(\"#Description\").getNumberRows(),2).css(\"background-Color\", \"lightgreen\");\n      $(\"#Description\").setValue(\"اطلاعات اصلاح شده مربوط به قرارداد / الحاقيه مردود شد و ثبت نگرديد .\",$(\"#Description\").getNumberRows(),2);\n      $('#Reg_Confirmation').find(\"span.textlabel\").css(\"background-color\", \"\");\n      break; \n    \n    case (RC == 1 && SRT == 2):\n      $('#Description').getControl($(\"#Description\").getNumberRows(),2).css(\"background-Color\", \"lightgreen\");\n      $(\"#Description\").setValue(\"اطلاعات اصلاح شده مربوط به قرارداد / الحاقيه تاييد شد و ثبت گرديد .\",$(\"#Description\").getNumberRows(),2);\n      $('#Reg_Confirmation').find(\"span.textlabel\").css(\"background-color\", \"\");\n      break;\n    \n    case (RC == 0 && SRT == 3):\n      $('#Description').getControl($(\"#Description\").getNumberRows(),2).css(\"background-Color\", \"lightgreen\");\n      $(\"#Description\").setValue(\"اطلاعات نيازمند به حذف مربوط به قرارداد / الحاقيه مردود شد و ثبت نگرديد .\",$(\"#Description\").getNumberRows(),2);\n      $('#Reg_Confirmation').find(\"span.textlabel\").css(\"background-color\", \"\");\n      break;   \n    \n    case (RC == 1 && SRT == 3):\n      $('#Description').getControl($(\"#Description\").getNumberRows(),2).css(\"background-Color\", \"lightgreen\");\n      $(\"#Description\").setValue(\"اطلاعات نيازمند به حذف مربوط به قرارداد / الحاقيه تاييد شد و ثبت گرديد .\",$(\"#Description\").getNumberRows(),2);\n      $('#Reg_Confirmation').find(\"span.textlabel\").css(\"background-color\", \"\");\n      break;    \n    \n}"},"language":"en","externalLibs":"lib/CustomJavaScript/jquery.ui_.datepicker1.8.14-cc/scripts/jquery.ui.datepicker-cc.all.min.js,\nlib/CustomJavaScript/jquery.ui_.datepicker1.8.14-cc/styles/jquery.ui.datepicker1.8-all.css","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"اطلاع رساني وضعيت فرآيند","colSpan":12}],[{"type":"subtitle","id":"subtitle0000000002","label":"اطلاعات قرارداد / الحاقيه","colSpan":3},{"type":"text","variable":"Select_Reg_Type","var_uid":"957397460632ea8aa4a5658073833509","dataType":"string","protectedValue":false,"id":"Select_Reg_Type","name":"Select_Reg_Type","label":"نحوه ورود اطلاعات","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"Select_Reg_Type","colSpan":3},{"type":"hidden","variable":"txt_name","var_uid":"520812988631f187d938d84004270602","dataType":"string","protectedValue":false,"id":"txt_name","name":"txt_name","defaultValue":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"txt_name","colSpan":3},{"type":"text","variable":"Contract_Counter","var_uid":"8664855956327fb2f2c81e4095334474","dataType":"string","protectedValue":false,"id":"Contract_Counter","name":"Contract_Counter","label":"شمارنده قرارداد","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"disabled","operation":"","dbConnection":"workflow","dbConnectionLabel":"