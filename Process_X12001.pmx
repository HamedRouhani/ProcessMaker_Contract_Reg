tion\").getNumberRows(),'2').attr('disabled',false); \n    $(\"#Description\").setValue($(\"#txt_name\").getValue(),$(\"#Description\").getNumberRows(),'1');\n  };\n\n// بررسي اطلاعات مربوط به اطلاعات جانبي و تهيه كد قرارداد\n\n$(\"#submit0000000001\").hide();\n\n$(\"#Check_Button\").find(\"button\").click(function(Subject,Suppliers,Personnel,Projects,Amount,Location,D,CT,CP,CS,Y,CC,FVal,CVal,i,j,check,Counter) {\n  \n// بررسي نوع قرارداد و شخصيت \n  \n  CS = $(\"#Contract_Status\").getValue();\n  $(\"#submit0000000001\").hide();\n  \n  if (CS == \"\") {\n    \n    $('#Contract_Status').getControl().css(\"background-color\", \"pink\");\n    alert (\"وضعيت قرارداد را انتخاب كنيد !\"); \n    \n  } else {\n    \n    $('#Contract_Status').getControl().css(\"background-color\", \"lightgreen\");\n    \n  }\n  \n  \n// موضوع قرارداد \n  \n  Counter = 0;\n  check = \"Yes\";\n  Subject = $(\"#Subject\").getNumberRows();\n      i = 1; \n      while (i-1 < Subject){\n      \n      \t$(\"#Subject\").getControl(i,2).css(\"background-color\", \"\");\n        i++;\n      \n      }\n      \n      i = 1;\n      while (i-1 < Subject) {\n    \n      FVal = $(\"#Subject\").getValue(i,2);\n    \n      if (FVal == \"\") {\n        \n        alert( \"بايستي موضوع قرارداد براي رديف \" + i + \" انتخاب شود!\");\n        $('#Subject').getControl(i,2).css(\"background-color\", \"pink\");\n        check = \"No\";\n        \n      } else {\n      \n        j = i+1;\n      \n        while (j-1 < Subject){\n        \n          CVal = $(\"#Subject\").getValue(j,2);\n                \n          if (FVal == CVal){\n            alert(\"رديفهاي \" + i + \" و \" + j + \" براي موضوعات قرارداد مشابه هستند!\");\n            $('#Subject').getControl(i,2).css(\"background-color\", \"pink\");\n            $('#Subject').getControl(j,2).css(\"background-color\", \"pink\");\n            check = \"No\";\n          }\n        j++;\n        } \n\n      }\n    \n      i++;\n    }\n  \n  if (check == \"Yes\"){\n    i = 1; \n      while (i-1 < Subject){\n      \n      \t$(\"#Subject\").getControl(i,2).css(\"background-color\", \"lightgreen\");\n        i++;\n      \n      }\n    \n    Counter++;\n    \n  }\n\n// طرف قرارداد (تامين كنندگان)   \n  check = \"Yes\";\n  Suppliers = $(\"#Suppliers\").getNumberRows();\n      i = 1; \n      while (i-1 < Suppliers){\n      \n      \t$(\"#Suppliers\").getControl(i,2).css(\"background-color\", \"\");\n        i++;\n      \n      }\n      \n      i = 1;\n      while (i-1 < Suppliers) {\n    \n      FVal = $(\"#Suppliers\").getValue(i,2);\n    \n      if (FVal == \"\") {\n        \n        alert( \"بايستي طرف قرارداد (تامين كنندگان) براي رديف \" + i + \" انتخاب شود!\");\n        $('#Suppliers').getControl(i,2).css(\"background-color\", \"pink\");\n        check = \"No\";\n        \n      } else {\n      \n        j = i+1;\n      \n        while (j-1 < Suppliers){\n        \n          CVal = $(\"#Suppliers\").getValue(j,2);\n                \n          if (FVal == CVal){\n            alert(\"رديفهاي \" + i + \" و \" + j + \" براي طرف قرارداد (تامين كنندگان) مشابه هستند!\");\n            $('#Suppliers').getControl(i,2).css(\"background-color\", \"pink\");\n            $('#Suppliers').getControl(j,2).css(\"background-color\", \"pink\");\n            check = \"No\";\n          }\n        j++;\n        } \n\n      }\n    \n      i++;\n    }\n  \n  if (check == \"Yes\"){\n    i = 1; \n      while (i-1 < Suppliers){\n      \n      \t$(\"#Suppliers\").getControl(i,2).css(\"background-color\", \"lightgreen\");\n        i++;\n     \n      }\n    \n    Counter++;\n    \n  }\n  \n//انتخاب شغل\n  check = \"Yes\";\n  Personnel = $(\"#Personnel\").getNumberRows();\n      i = 1; \n      while (i-1 < Personnel){\n      \n      \t$(\"#Personnel\").getControl(i,3).css(\"background-color\", \"\");\n        i++;\n      \n      }\n      \n      i = 1;\n      while (i-1 < Personnel) {\n    \n      FVal = $(\"#Personnel\").getValue(i,3);\n    \n      if (FVal == \"\") {\n        \n        alert( \"بايستي شغل فرد براي رديف \" + i + \" انتخاب شود!\");\n        $('#Personnel').getControl(i,3).css(\"background-color\", \"pink\");\n        check = \"No\";\n        \n      } else {\n      \n        j = i+1;\n      \n        while (j-1 < Personnel){\n        \n          CVal = $(\"#Personnel\").getValue(j,3);\n                \n          if (FVal == CVal){\n            alert(\"رديفهاي \" + i + \" و \" + j + \" براي شغل فرد مشابه هستند!\");\n            $('#Personnel').getControl(i,3).css(\"background-color\", \"pink\");\n            $('#Personnel').getControl(j,3).css(\"background-color\", \"pink\");\n            check = \"No\";\n          }\n        j++;\n        } \n\n      }\n    \n      i++;\n    }\n  \n  if (check == \"Yes\"){\n    i = 1; \n      while (i-1 < Personnel){\n      \n      \t$(\"#Personnel\").getControl(i,3).css(\"background-color\", \"lightgreen\");\n        i++;\n      \n      }\n    \n    Counter++;\n    \n  }\n  \n// پروژه‌هاي مرتبط با قرارداد \n  \n  check = \"Yes\";\n  Projects = $(\"#Projects\").getNumberRows();\n      i = 1; \n      while (i-1 < Projects){\n      \n      \t$(\"#Projects\").getControl(i,2).css(\"background-color\", \"\");\n        i++;\n      \n      }\n      \n      i = 1;\n      while (i-1 < Projects) {\n    \n      FVal = $(\"#Projects\").getValue(i,2);\n    \n      if (FVal == \"\") {\n        \n        alert( \"بايستي طرف پروژه‌هاي مرتبط با قرارداد براي رديف \" + i + \" انتخاب شود!\");\n        $('#Projects').getControl(i,2).css(\"background-color\", \"pink\");\n        check = \"No\";\n        \n      } else {\n      \n        j = i+1;\n      \n        while (j-1 < Projects){\n        \n          CVal = $(\"#Projects\").getValue(j,2);\n                \n          if (FVal == CVal){\n            alert(\"رديفهاي \" + i + \" و \" + j + \" براي پروژه‌هاي مرتبط با قرارداد مشابه هستند!\");\n            $('#Projects').getControl(i,2).css(\"background-color\", \"pink\");\n            $('#Projects').getControl(j,2).css(\"background-color\", \"pink\");\n            check = \"No\";\n          }\n        j++;\n        } \n\n      }\n    \n      i++;\n    }\n  \n  if (check == \"Yes\"){\n    i = 1; \n      while (i-1 < Projects){\n      \n      \t$(\"#Projects\").getControl(i,2).css(\"background-color\", \"lightgreen\");\n        i++;\n      \n      }\n    \n    Counter++;\n    \n  }\n  \n// Amount\n  \ncheck = \"Yes\";\n  Amount = $(\"#Amount\").getNumberRows();\n      i = 1; \n      while (i-1 < Amount){\n      \n      \t$(\"#Amount\").getControl(i,1).css(\"background-color\", \"\");\n        i++;\n      \n      }\n      \n      i = 1;\n      while (i-1 < Amount) {\n    \n      FVal = $(\"#Amount\").getValue(i,1);\n    \n      if (FVal == \"\") {\n        \n        alert( \"بايستي واحد پولي براي رديف \" + i + \" انتخاب شود!\");\n        $('#Amount').getControl(i,1).css(\"background-color\", \"pink\");\n        check = \"No\";\n        \n      } else {\n      \n        j = i+1;\n      \n        while (j-1 < Amount){\n        \n          CVal = $(\"#Amount\").getValue(j,1);\n                \n          if (FVal == CVal){\n            alert(\"رديفهاي \" + i + \" و \" + j + \" براي واحدهاي پولي مشابه هستند!\");\n            $('#Amount').getControl(i,1).css(\"background-color\", \"pink\");\n            $('#Amount').getControl(j,1).css(\"background-color\", \"pink\");\n            check = \"No\";\n          }\n        j++;\n        } \n\n      }\n    \n      i++;\n    }\n  \n    if (check == \"Yes\"){\n    i = 1; \n      while (i-1 < Amount){\n      \n      \t$(\"#Amount\").getControl(i,1).css(\"background-color\", \"lightgreen\");\n        i++;\n      \n      }\n      \n      Counter++;\n      \n    }\n  \n  check = \"Yes\";\n  Amount = $(\"#Amount\").getNumberRows();\n      i = 1; \n      while (i-1 < Amount){\n      \n      \t$(\"#Amount\").getControl(i,2).css(\"background-color\", \"\");\n        i++;\n      \n      }\n      \n      i = 1;\n      while (i-1 < Amount) {\n    \n      FVal = $(\"#Amount\").getValue(i,2);\n    \n      if (FVal == \"\") {\n        \n        alert( \"بايستي مبلغ براي رديف \" + i + \" درج شود!\");\n        $('#Amount').getControl(i,2).css(\"background-color\", \"pink\");\n        check = \"No\";\n\n      }\n    \n      i++;\n    }\n  \n  if (check == \"Yes\"){\n    i = 1; \n      while (i-1 < Amount){\n      \n      \t$(\"#Amount\").getControl(i,2).css(\"background-color\", \"lightgreen\");\n        i++;\n      \n      }\n    Counter++;\n  }\n  \n// Location\n  check = \"Yes\";\n  Location = $(\"#Location\").getNumberRows();\n      i = 1; \n      while (i-1 < Location){\n      \n      \t$(\"#Location\").getControl(i,2).css(\"background-color\", \"\");\n        i++;\n      \n      }\n      \n      i = 1;\n      while (i-1 < Location) {\n    \n      FVal = $(\"#Location\").getValue(i,2);\n    \n      if (FVal == \"\") {\n        \n        alert( \"بايستي محلهاي مرتبط با قرارداد براي رديف \" + i + \" انتخاب شود!\");\n        $('#Location').getControl(i,2).css(\"background-color\", \"pink\");\n        check = \"No\";\n        \n      } else {\n      \n        j = i+1;\n      \n        while (j-1 < Location){\n        \n          CVal = $(\"#Location\").getValue(j,2);\n                \n          if (FVal == CVal){\n            alert(\"رديفهاي \" + i + \" و \" + j + \" براي محلهاي مرتبط با قرارداد مشابه هستند!\");\n            $('#Location').getControl(i,2).css(\"background-color\", \"pink\");\n            $('#Location').getControl(j,2).css(\"background-color\", \"pink\");\n            check = \"No\";\n          }\n        j++;\n        } \n\n      }\n    \n      i++;\n    }\n  \n  if (check == \"Yes\"){\n    i = 1; \n      while (i-1 < Location){\n      \n      \t$(\"#Location\").getControl(i,2).css(\"background-color\", \"lightgreen\");\n        i++;\n      \n      }\n    \n    Counter++;\n    \n  }\n  \n  // بررسي شرح\n  \n    D =  $(\"#Description\").getValue($(\"#Description\").getNumberRows(),2);\n  \tswitch (true) {\n   \n    case ( D == \"\"):\n      $('#Description').getControl($(\"#Description\").getNumberRows(),2).css(\"background-Color\", \"pink\");\n      alert (\"نكات اصلاحي را وارد نماييد ...\");\n      break;\n        \n    case ( D != \"\"):\n      $('#Description').getControl($(\"#Description\").getNumberRows(),2).css(\"background-Color\", \"lightgreen\");\n      Counter++;\n      break;        \n      \n  }\n  \n  //  نمايش دكمه ثبت اطلاعات و توليد كد قرارداد\n  \n  if (Counter == 8) {\n    \n    $(\"#submit0000000001\").show();\n        \n  }\n  \n});\n  "},"language":"en","externalLibs":"lib/CustomJavaScript/jquery.ui_.datepicker1.8.14-cc/scripts/jquery.ui.datepicker-cc.all.min.js,\nlib/CustomJavaScript/jquery.ui_.datepicker1.8.14-cc/styles/jquery.ui.datepicker1.8-all.css","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"اصلاح اطلاعات قرارداد","colSpan":12}],[{"type":"text","variable":"Contract_Code","var_uid":"391784667626522866294f7007277540","dataType":"string","protectedValue":false,"id":"Contract_Code","name":"Contract_Code","label":"كد قرارداد","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"disabled","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"Contract_Code","colSpan":3},{"type":"textarea","variable":"Contract_Title","var_uid":"554962158631f176c9a0bb4007755584","dataType":"string","protectedValue":false,"id":"Contract_Title","name":"Contract_Title","label":"عنوان قرارداد","defaultValue":"","placeholder":"","hint":"","required":false,"validate":"","validateMessage":"","mode":"parent","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","rows":"2","var_name":"Contract_Title","colSpan":9}],[{"type":"text","variable":"Budget_Number","var_uid":"8054809576379c50d0ede37021619802","dataType":"string","protectedValue":false,"id":"Budget_Nu